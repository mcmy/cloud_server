<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nfcat.cloud.sql.mapper.NfConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.nfcat.cloud.sql.entity.NfConfig">
        <id column="id" property="id" />
        <result column="conf_name" property="confName" />
        <result column="conf_value" property="confValue" />
        <result column="info" property="info" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, conf_name, conf_value, info
    </sql>

</mapper>
