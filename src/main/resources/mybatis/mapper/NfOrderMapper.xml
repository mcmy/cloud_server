<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nfcat.cloud.sql.mapper.NfOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.nfcat.cloud.sql.entity.NfOrder">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="shop_info" property="shopInfo" />
        <result column="price_json" property="priceJson" />
        <result column="create_time" property="createTime" />
        <result column="finish_time" property="finishTime" />
        <result column="destroy_time" property="destroyTime" />
        <result column="state" property="state" />
        <result column="info" property="info" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, shop_info, price_json, create_time, finish_time, destroy_time, state, info
    </sql>

</mapper>
